                <form id="logonForm" name="logonForm" action="/" method="post">
                  <!-- Username form field -->
                  <div class="form-group">
                    <div class="input-group">
                      <span class="input-group-addon"
                        ><span
                          class="glyphicon glyphicon-envelope"
                          aria-hidden="true"
                        ></span
                      ></span>
                      <label
                        class="control-label sr-only"
                        for="j_username"
                        style="font-size: 18px"
                      >
                        UsernameUsername
                        <span class="mandatory_field fc-inline">*</span>
                      </label>
                      <input
                        class="form-control input-lg"
                        id="j_username"
                        autocomplete="off"
                        type="text"
                        maxlength="60"
                        placeholder="Enter email address"
                        name="email"
                        value=""
                      />
                    </div>
                  </div>

                  <!-- Password form field -->
                  <div class="form-group">
                    <div class="input-group">
                      <span class="input-group-addon"
                        ><span
                          class="glyphicon glyphicon-lock"
                          aria-hidden="true"
                        ></span
                      ></span>
                      <label
                        class="control-label sr-only"
                        for="j_password"
                        style="font-size: 18px"
                      >
                        PasswordPassword
                        <span class="mandatory_field fc-inline">*</span>
                      </label>
                      <input
                        id="j_password"
                        class="form-control input-lg"
                        autocomplete="off"
                        type="password"
                        name="password"
                        maxlength="20"
                        placeholder="Enter Password"
                      />
                    </div>

                    <input type="hidden" name="fromUrl" id="fromUrl" value="" />

                    <input
                      type="hidden"
                      name="csTok"
                      id="csTok"
                      value="2ac1d1c3-a4cf-453f-9989-a2e74e5c86df"
