    <form id="cSSCreateAccountCommand" name="createAccountForm" action="/personal" method="post">
            
        <div class="row">
            <div class="col-md-12">
                
                
                <div class="shadow-panel">
                    <div class="header section-header">
                        Personal Information
                    </div>
                    <div class="content">
                        <div class="row">
                            
                                <div class="col-md-6 form-group middle ">
                                    <div class="col-md-4"><label for="fname" class="control-label fc-inline">First Name<span class="mandatory_field">*</span> </label></div>
                                        <div class="col-md-8">
                                            <input type="text" tabindex="8" id="fname" name="fName" alt="firstName" value="" maxlength="50" class="form-control fc-inline">
                                        </div>
                                    
                                </div>
                            
                            
                                <div class="col-md-6 form-group middle ">
                                    <div class="col-md-4"><label for="mi" class="control-label fc-inline">Middle Initial</label></div>
                                    <div class="col-md-8"><input type="text" tabindex="9" maxlength="1" name="mName" alt="middleName" size="2" value="" class="form-control fc-inline">
                                    </div>
                                    
                                </div>
                            
                            
                                <div class="col-md-6 form-group middle ">
                                    <div class="col-md-4"><label for="lname" class="control-label fc-inline">Last Name  <span class="mandatory_field">*</span> </label></div>
                                        <div class="col-md-8">
                                            <input type="text" tabindex="10" name="lName" id="lname" value="" alt="lName" maxlength="50" class="form-control fc-inline">
                                        </div>
                                    
                                </div>
                            
                            
                                <div class="col-md-6 form-group middle ">
                                    <div class="col-md-4"><label for="" class="control-label fc-inline">Suffix </label></div>
                                    <div class="col-md-8"><select name="suffix" id="nameSuffix" class="form-control fc-inline" tabindex="11">
    <option value=" " selected="selected" > </option>
    <option value="SR">Sr.</option>
    <option value="JR">Jr.</option>
    <option value="III">III</option>
    <option value="CPA">Certified Public Accountant</option>
    <option value="DDS">Doctor of Dental Surgery</option>
    <option value="MD">Doctor of Medicine</option>
    <option value="RN">Registered Nurse</option>
    <option value="INC">Incorporated</option>
    <option value="ED">Doctor of Education</option>
    <option value="PHD">Doctor of Philosophy</option>
    <option value="II">II</option>
    
    </select>
    
                                    </div>
                                    
                                </div>
                            
                            
                        <!-- SSN DATA -->
                        
                            
                                
                            
                            
                                
                            
                            
                                
                            
                            <div class="col-md-6 form-group middle ">
