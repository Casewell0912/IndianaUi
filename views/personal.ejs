                                    <div class="input-group">
                                            <input type="text" tabindex="19" size="10" id="birthDate" name="dob" value="" class="form-control datepicker-mycal hasDatepicker" maxlength="10" alt="birthDate" autocomplete="off" placeholder="mm/dd/yyyy">
                                            
                                            <span class="input-group-addon calendar datepicker-btn1" id="btnPicker">
                                                <a style="text-decoration:none;cursor: pointer;"><i class="glyphicon glyphicon-calendar"></i></a>
                                            </span>
                                        </div>
                                        <span class="help-block">
                                            (mm/dd/yyyy)
                                        </span>
                                    </div>
                                </div>
                            
                        </div>
                        <div class="clearfix col-md-block"></div>
                    </div>
                </div>
            </div>
